{
  "name": "google-fonts-plugin",
  "version": "4.1.0",
  "description": "Webpack plugin that downloads fonts from Google Fonts and encodes them to base64.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "pretest": "standard --fix",
    "test": "jest --coverage --env=node",
    "posttest": "codecov",
    "test-watch": "jest --watch --env=node",
    "build": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SirPole/google-fonts-plugin"
  },
  "keywords": [
    "google",
    "fonts",
    "google-fonts",
    "webpack-plugin",
    "webpack"
  ],
  "author": "Martin Brychta <martin@brychta.name> (https://www.brychta.name/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SirPole/google-fonts-plugin/issues"
  },
  "homepage": "https://github.com/SirPole/google-fonts-plugin#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true,
      "jest": true
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "find-cache-dir": "^3.0.0",
    "neon-js": "^1.1.2",
    "pkg-up": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "codecov": "^3.5.0",
    "jest": "^24.8.0",
    "standard": "^12.0.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  }
}
