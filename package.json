{
  "name": "google-fonts-plugin",
  "version": "2.0.2",
  "description": "Webpack plugin that downloads fonts from Google Fonts and encodes them to base64.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "pretest": "standard",
    "test": "jest --coverage --env=node",
    "posttest": "codecov",
    "test-watch": "jest --watch --env=node",
    "build": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SirPole/google-fonts-plugin"
  },
  "keywords": [
    "google",
    "fonts",
    "google-fonts",
    "webpack-plugin",
    "webpack"
  ],
  "author": "Martin Brychta <martin@brychta.name> (https://www.brychta.name/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SirPole/google-fonts-plugin/issues"
  },
  "homepage": "https://github.com/SirPole/google-fonts-plugin#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true,
      "jest": true
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cssnano": "^4.0.5",
    "mkdirp": "^0.5.1",
    "neon-js": "^1.1.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "codecov": "^3.0.4",
    "jest": "^23.4.2",
    "standard": "^11.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
